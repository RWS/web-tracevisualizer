<Window x:Class="SdlWeb_TraceVisualizer.ProcessList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ProcessList" Height="300" Width="300" Loaded="ProcessList_OnLoaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <ListBox Name="ProcessListBox" SelectionMode="Extended"/>
        <Button Content="OK" Grid.Row="1" Name="OkButton" Height="20" Width="100" Margin="89,0,104.6,0" IsDefault="True" Click="OkButton_Click"/>
        <Button Content="Cancel" Grid.Row="1" Name="CancelButton" Height="20" Width="100" Margin="194,0,-0.4,0" Click="CancelButton_Click"/>

    </Grid>
</Window>
